#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/feng/gateway/code/node_modules/.pnpm/ts-jest@29.3.2_@babel+core@7.27.1_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_d8a0bb9e0213e9e6744c3f8029e750d8/node_modules/ts-jest/node_modules:/Users/feng/gateway/code/node_modules/.pnpm/ts-jest@29.3.2_@babel+core@7.27.1_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_d8a0bb9e0213e9e6744c3f8029e750d8/node_modules:/Users/feng/gateway/code/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/feng/gateway/code/node_modules/.pnpm/ts-jest@29.3.2_@babel+core@7.27.1_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_d8a0bb9e0213e9e6744c3f8029e750d8/node_modules/ts-jest/node_modules:/Users/feng/gateway/code/node_modules/.pnpm/ts-jest@29.3.2_@babel+core@7.27.1_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_d8a0bb9e0213e9e6744c3f8029e750d8/node_modules:/Users/feng/gateway/code/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
