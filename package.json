    "@babel/runtime": "^7.27.0",
    "@connectis/diff-test-coverage": "^1.5.3",
    "@improbable-eng/grpc-web": "^0.13.0",
    "@types/app-root-path": "^1.2.8",
    "@types/bn.js": "5.1.6",
    "@types/bs58": "^4.0.4",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/level": "^6.0.3",
    "@types/mathjs": "^9.4.2",
    "@types/minimist": "^1.2.5",
    "@types/node": "^15.14.9",
    "@types/node-fetch": "^2.6.12",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.18.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "bs58": "^4.0.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-formatter-table": "^7.32.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^2.1.1",
    "google-protobuf": "^3.21.4",
    "hardhat": "^2.22.19",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-extended": "^0.11.5",
    "jsbi": "^3.2.5",
    "mock-ethers-provider": "^1.0.2",
    "node-gyp": "^11.2.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^3.0.2",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "viem": "^0.3.50"
  },
  "resolutions": {
    "@types/bn.js": "5.1.6",
    "bn.js": "5.2.1",
    "web3-utils": "1.7.3",
    "brotli": "1.3.2",
    "minimist": "^0.2.4",
    "mkdirp": "^1.0.4",
    "@openzeppelin/contracts": "^4.9.6"
  },
  "files": [
    "/dist",
    "/src/commands"
  ],
  "oclif": {
    "bin": "gateway",
    "dirname": "gateway",
    "commands": "./dist/src/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ],
    "topicSeparator": " ",
    "topics": {
      "balance": {
        "description": "Retrieve token balances for a wallet on the specified chain"
      },
      "start": {
        "description": "Start the Gateway API server"
      },
      "build": {
        "description": "Build Gateway by compiling Typescript into Javascript"
      }
    }
  },
  "bin": {
    "gateway": "./dist/index.js"
  }
}
