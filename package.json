    "bn.js": "5.2.1",
    "web3-utils": "1.7.3"
  },
  "files": [
    "/dist",
    "/src/commands"
  ],
  "oclif": {
    "bin": "gateway",
    "dirname": "gateway",
    "commands": "./dist/src/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ],
    "topicSeparator": " ",
    "topics": {
      "balance": {
        "description": "Retrieve token balances for a wallet on the specified chain"
      },
      "start": {
        "description": "Start the Gateway API server"
      },
      "build": {
        "description": "Build Gateway by compiling Typescript into Javascript"
      }
    }
  },
  "bin": {
    "gateway": "./dist/src/index.js"
  }
}
