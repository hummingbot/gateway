{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "allowedSlippage": { "type": "string" },
    "strategyType": { "type": "number" },
    "pools": {
      "type": "object",
      "patternProperties": {
        "^[A-Z]+-[A-Z]+$": {
          "type": "string"
        }
      },
      "additionalProperties": false
    }
  },
  "required": ["allowedSlippage", "pools", "strategyType"],
  "additionalProperties": false
}
