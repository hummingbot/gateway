{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "networks": {
      "type": "object",
      "patternProperties": {
        "^\\w+$": {
          "type": "object",
          "properties": {
            "chainId": { "type": "string" },
            "nodeURL": { "type": "string" },
            "tokenListType": {
              "type": "string"
            },
            "tokenListSource": {
              "type": "string"
            },
            "marketListType": {
              "type": "string"
            },
            "marketListSource": {
              "type": "string"
            }
          },
          "required": ["chainId", "nodeURL"],
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    "nativeCurrencySymbol": { "type": "string" },
    "gasLimitEstimate": { "type": "number" },
    "prefix": { "type": "string" },
    "gasPrice": { "type": "number" },
    "gasPriceSuffix": { "type": "string" },
    "manualGasPrice": { "type": "number" }
  },
  "additionalProperties": false,
  "required": ["nativeCurrencySymbol"]
}
