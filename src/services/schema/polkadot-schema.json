{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "networks": {
            "type": "object",
            "patternProperties": {
                "^\\w+$": {
                    "type": "object",
                    "properties": {
                        "chainId": {
                            "type": "string"
                        },
                        "nodeURL": {
                            "type": "string"
                        },
                        "tokenListType": {
                            "type": "string"
                        },
                        "tokenListSource": {
                            "type": "string"
                        },
                        "nativeCurrencySymbol": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "nodeURL",
                        "tokenListType",
                        "tokenListSource",
                        "nativeCurrencySymbol"
                    ],
                    "additionalProperties": false
                }
            },
            "additionalProperties": false
        },
        "gasLimitEstimate": {
            "type": "number"
        }
    },
    "additionalProperties": false,
    "required": [
        "networks"
    ]
}