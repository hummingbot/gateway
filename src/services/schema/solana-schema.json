{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "networks": {
        "type": "object",
        "patternProperties": {
          "^..+$": {
            "type": "object",
            "properties": {
              "nodeURL": { "type": "string" },
              "tokenListType": { "type": "string" },
              "tokenListSource": { "type": "string" },
              "nativeCurrencySymbol": { "type": "string" },
              "maxLRUCacheInstances": { "type": "number" }
            },
            "required": [
              "nodeURL",
              "tokenListType",
              "tokenListSource",
              "nativeCurrencySymbol",
              "maxLRUCacheInstances"
            ],
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      },
      "network": { "type": "string" },
      "tokenProgram": { "type": "string" },
      "transactionLamports": { "type": "number" },
      "lamportsToSol": { "type": "number" },
      "timeToLive": { "type": "number" }
    },
    "additionalProperties": false
  }
  