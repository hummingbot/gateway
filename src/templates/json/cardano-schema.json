{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "networks": {
      "type": "object",
      "patternProperties": {
        "^\\w+$": {
          "type": "object",
          "properties": {
            "tokenListType": { "type": "string" },
            "tokenListSource": { "type": "string" },
            "nativeCurrencySymbol": { "type": "string" },
            "apiurl": { "type": "string" },
            "projectId": { "type": "string" }
          },
          "required": [
            "tokenListType",
            "tokenListSource",
            "nativeCurrencySymbol",
            "apiurl",
            "projectId"
          ],
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
